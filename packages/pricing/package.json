{
  "name": "pricing",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec 'ts-node src/index.ts'",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "proto": "pbjs -t static-module -w es6 -o src/takerPricing_pb.js src/takerPricing.proto && pbts -o src/takerPricing_pb.d.ts src/takerPricing_pb.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "protobufjs": "^8.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "nodemon": "^3.1.9",
    "protobufjs-cli": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
